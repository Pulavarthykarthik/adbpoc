				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": "SELECT DISTINCT T.TABLE_SCHEMA,T.TABLE_NAME,concat(t.table_schema,'.',t.table_name) as Schema_Table_Name \r\nFROM INFORMATION_SCHEMA.TABLES t \r\ninner join information_schema.columns c on t.table_schema=c.table_schema \r\nand t.table_name = c.table_name \r\nWHERE C.COLUMN_NAME IN ('DTS_DW_UPDT') \r\nand T.TABLE_NAME NOT LIKE ('%-%') \r\nAND T.TABLE_NAME NOT LIKE '%EXTERNAL' \r\nAND T.TABLE_NAME NOT LIKE '%EXT' \r\nAND T.TABLE_NAME <> 'accessory_ext_sch'\r\nAND T.TABLE_TYPE = 'BASE TABLE'\r\nORDER BY T.TABLE_NAME",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "AzureSynapseAnalyticsSQLDB",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "ForEachTableList_DW_UPDT_DT",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "LookupDW_UPDT_DTInInformationSchema",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('LookupDW_UPDT_DTInInformationSchema').output.value",
						"type": "Expression"
					},
					"batchCount": 20,
					"activities": [
						{
							"name": "Copy data1",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "SqlDWSource",
									"sqlReaderQuery": {
										"value": "select * from (select distinct count(*) as Row_Count,cast(DW_UPDT_DT as date) as \"Last_Update_date\", '@{item().Schema_Table_Name}' as \"Table_Name\",ROW_NUMBER() OVER(ORDER BY cast(DW_UPDT_DT as date) desc) AS Row# from @{item().Schema_Table_Name} group by cast(DW_UPDT_DT as date))a where a.Row# <=3 order by a.Last_Update_date desc",
										"type": "Expression"
									},
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"typeConversion": true,
									"typeConversionSettings": {
										"allowDataTruncation": true,
										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
