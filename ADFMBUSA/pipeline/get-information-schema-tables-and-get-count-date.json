{
	"name": "get-information-schema-tables-and-get-count-date",
	"properties": {
		"activities": [
			{
				"name": "LookupDW_UPDT_DTInInformationSchema",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "DeleteOldOutputFiles",
						"dependencyConditions": [
							"Completed"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlDWSource",
						"sqlReaderQuery": "SELECT DISTINCT T.TABLE_SCHEMA,T.TABLE_NAME,\r\nconcat(t.table_schema,'.',t.table_name) as Schema_Table_Name \r\nFROM INFORMATION_SCHEMA.TABLES t \r\ninner join information_schema.columns c on t.table_schema=c.table_schema \r\nand t.table_name = c.table_name \r\nWHERE C.COLUMN_NAME IN ('DW_UPDT_DT') \r\nand T.TABLE_NAME NOT LIKE '%-%' \r\nAND T.TABLE_NAME NOT LIKE '%EXTERNAL' \r\nAND T.TABLE_NAME NOT LIKE '%EXT'\r\nAND T.TABLE_TYPE = 'BASE TABLE'",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "AzureSynapseAnalyticsSQLDB",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "LookupDTS_DW_UPDTInInformationSchema",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "DeleteOldOutputFiles",
