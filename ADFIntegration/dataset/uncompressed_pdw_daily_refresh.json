{
	"name": "uncompressed_pdw_daily_refresh",
	"properties": {
		"linkedServiceName": {
			"referenceName": "Gen2_Domain_ADLS",
			"type": "LinkedServiceReference"
		},
		"folder": {
			"name": "PDW"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileSystem": {
					"value": "@concat('pdw-edw/prod/data/year=',substring(formatDateTime(utcnow(),'yyyy-MM-dd'),0,4),'/month=',substring(formatDateTime(utcnow(),'yyyy-MM-dd'),5,2),'/day=',substring(formatDateTime(utcnow(),'yyyy-MM-dd'),8,2))",
					"type": "Expression"
				}
			},
			"compression": {
				"type": "ZipDeflate",
				"level": "Fastest"
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}