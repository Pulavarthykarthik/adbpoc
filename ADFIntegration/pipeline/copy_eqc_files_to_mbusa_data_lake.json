{
	"name": "copy_eqc_files_to_mbusa_data_lake",
	"properties": {
		"description": "This pipeline will copy EQC lead files from domain data lake to MBUSA data lake. CPS-EDW container is used as a temporary solution. ",
		"activities": [
			{
				"name": "copy EQC lead file",
				"description": "This activity will copy the zip file from domain data lake to MBUSA data lake.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "DAPS+MS+SALESFORCE+EQLEADS+*",
							"deleteFilesAfterCompletion": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings",
							"compressionProperties": {
								"type": "ZipDeflateReadSettings"
							}
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "eqc_file_source",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "eqc_file_dest",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "SALESFORCE"
		},
		"annotations": []
	}
}