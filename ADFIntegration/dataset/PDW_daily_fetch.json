{
	"name": "PDW_daily_fetch",
	"properties": {
		"linkedServiceName": {
			"referenceName": "Gen2_Domain_ADLS",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"run_date": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileSystem": {
					"value": "@concat('pdw-edw/prod/data/year=',substring(dataset().run_date,0,4),'/month=',substring(dataset().run_date,5,2),'/day=',substring(dataset().run_date,8,2))",
					"type": "Expression"
				}
			},
			"compression": {
				"type": "ZipDeflate",
				"level": "Fastest"
			}
		}
	}
}